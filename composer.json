{
    "name": "fyennyi/alerts-in-ua-php",
    "description": "PHP client library for the alerts.in.ua API.",
    "type": "library",
    "license": "LicenseRef-CSSM-Unlimited-2.0",
    "require": {
        "guzzlehttp/guzzle": "^7.9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1.2",
        "phpdocumentor/phpdocumentor": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Fyennyi\\AlertsInUa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fyennyi\\AlertsInUa\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "docs": "phpdoc -d src -t docs"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
