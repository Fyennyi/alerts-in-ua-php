{
    "name": "fyennyi/alerts-in-ua-php",
    "description": "PHP client library for the alerts.in.ua API.",
    "type": "library",
    "license": "LicenseRef-CSSM-Unlimited-2.0",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "symfony/cache": "^7.3 || ^8.0",
        "psr/simple-cache": "^3.0",
        "fyennyi/async-cache-php": "^1.0",
        "fyennyi/nominatim-async": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.5",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^2.1",
        "php-vcr/php-vcr": "^1.8",
        "php-vcr/phpunit-testlistener-vcr": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Fyennyi\\AlertsInUa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
